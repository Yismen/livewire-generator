{
    "name": "dainsys/livewire-generator",
    "description": "Livewire files generators",
    "require": {
        "php" : ">=7.0",
        "illuminate/filesystem": "^7.0||^8.0",
        "illuminate/console": "^7.0||^8.0",
        "illuminate/support": "^7.0||^8.0",
        "livewire/livewire": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^9.3.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Yismen Jorge",
            "email": "yjorge@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Dainsys\\LivewireGenerator\\": "src/"
        }
    },
    "autoload-dev":  {
        "psr-4": {
            "Dainsys\\LivewireGenerator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dainsys\\LivewireGenerator\\LivewireGeneratorServiceProvider"
            ]
        }
    }
}
